    									</xf:item>
    									<xf:item>
    										<xf:label><![CDATA[new row]]></xf:label>
    										<xf:value></xf:value>
    									</xf:item>
    									<xf:item>
    										<xf:label><![CDATA[new row]]></xf:label>
